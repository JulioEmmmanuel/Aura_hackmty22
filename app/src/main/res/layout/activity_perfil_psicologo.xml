<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="@dimen/padding_layout"
    android:paddingRight="@dimen/padding_layout"
    android:paddingTop="@dimen/padding_layout"
    tools:context=".PerfilPsicologo">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/margin_t_text"
        android:text="@string/namePaulina"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="16dp"
        tools:visibility="visible" />


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="157dp"
        android:layout_height="133dp"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/margin_t_text"
        app:srcCompat="@drawable/psic1"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="45dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="334dp"
        android:layout_height="345dp"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/margin_t_text"
        android:text="@string/infoPaulina"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="179dp" />

</LinearLayout>